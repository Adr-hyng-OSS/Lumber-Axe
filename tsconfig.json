    "allowJs": true,
    "checkJs": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "rootDir": "./src",
    "outDir": "./BP/scripts"
  },
  "exclude": [ "node_modules"],
  "include": [ "src/**/*"]
}