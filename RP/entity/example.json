{
	"format_version": "1.10.0",
	"minecraft:client_entity": {
		"description": {
			"identifier": "outlined_entities:example",
			"materials": {
				"base": "example.base",
				"outline": "example.outline"
			},
			"textures": {
				"default": "textures/entity/example"
			},
			"geometry": {
				"default": "geometry.example"
			},
			"render_controllers": [
				"controller.render.example.base",
				{"controller.render.example.outline": "query.variant"}
			],
			
			"scripts": {
				"pre_animation": [
					"query.mark_variant == 0 ? {t.red = 0.906; t.green = 0.184; t.blue = 0.102;};",
					"query.mark_variant == 1 ? {t.red = 0.075; t.green = 0.369; t.blue = 0.722;};",
					"query.mark_variant == 2 ? {t.red = 0.937; t.green = 0.816; t.blue = 0.231;};",
					"query.mark_variant == 3 ? {t.red = 0.271; t.green = 0.973; t.blue = 0.306;};"
				],
				
				"animate": ["spin"]
			},
			"animations": {
				"spin": "animation.example.spin"
			}
		}
	}
}